    "gsap": "^3.12.5",
    "js-confetti": "^0.12.0",
    "pagefind": "^1.3.0",
    "react": "^18.3.1",
    "react-compiler-runtime": "^19.0.0-beta-63e3235-20250105",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "svgo": "^3.3.2",
    "swiper": "^11.1.14",
    "tsparticles": "^2.12.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
