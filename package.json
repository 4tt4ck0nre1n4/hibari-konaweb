{
  "name": "hibari-konaweb",
  "type": "module",
  "version": "0.0.1",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.astro"
  ],
  "scripts": {
    "copy": "node -e \"require('fs-extra').copySync('./app/public/wp-content/uploads', './public/wp-content/uploads')\"",
    "clean": "node -e \"const fs = require('fs-extra'); const path = require('path'); const dirs = ['.vite-cache', 'node_modules/.vite', 'node_modules/.astro', 'dist', '.astro']; dirs.forEach(dir => { try { const fullPath = path.resolve(process.cwd(), dir); if (fs.existsSync(fullPath)) { fs.removeSync(fullPath); console.log('Cleaned:', dir); } else { console.log('Not exists:', dir); } } catch (e) { console.log('Failed:', dir, e.message); } });\"",
    "dev": "npm run copy && astro dev",
    "dev:clean": "npm run clean && npm run dev",
    "start": "astro dev",
    "build": "npm run copy && astro build && pagefind --site dist",
    "build:check": "npm run copy && astro check && astro build && pagefind --site dist",
    "build:clean": "npm run clean && npm run build",
    "format": "prettier --write 'src/**/*.{js,ts,jsx,tsx,.d.ts,astro,css,scss}'",
    "eslint": "eslint . --config eslint.config.mjs",
    "stylelint": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "lint": "npm run eslint && npm run stylelint",
    "postbuild": "pagefind --site dist/ --output-subdir pagefind",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^6.5.11",
    "@astrojs/node": "^9.4.3",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.5.1",
    "@fontsource/poppins": "^5.2.6",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@hookform/resolvers": "^3.9.0",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/devicon": "^1.2.6",
    "@iconify-json/emojione-v1": "^1.2.0",
    "@iconify-json/fa6-brands": "^1.2.2",
    "@iconify-json/fa6-solid": "^1.2.2",
    "@iconify-json/flat-color-icons": "^1.2.1",
    "@iconify-json/fluent-emoji": "^1.2.1",
    "@iconify-json/fluent-emoji-high-contrast": "^1.2.1",
    "@iconify-json/ic": "^1.2.1",
    "@iconify-json/ion": "^1.2.2",
    "@iconify-json/logos": "^1.2.3",
    "@iconify-json/octicon": "^1.2.1",
    "@iconify-json/solar": "^1.2.2",
    "@iconify-json/vscode-icons": "^1.2.24",
    "@iconify/json": "^2.2.244",
    "@iconify/react": "^6.0.0",
    "@pagefind/default-ui": "^1.3.0",
    "@rive-app/canvas": "^2.29.2",
    "@rive-app/react-canvas": "^4.21.2",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/shape-star": "^3.8.1",
    "@tsparticles/slim": "^3.8.1",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^5.13.5",
    "astro-icon": "^1.1.2",
    "astro-pagefind": "^1.8.3",
    "axios": "^1.7.2",
    "caniuse-lite": "^1.0.30001717",
    "dotenv": "^16.4.5",
