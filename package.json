    "sass-embedded": "^1.80.6",
    "stylelint": "^16.25.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.1",
    "ts-node": "^10.9.2",
    "vite-plugin-dynamic-import": "^1.6.0"
  }
}
